{
  "layout": {
    "name": "Isaac Robot - RealSense Cameras",
    "root": {
      "direction": "row",
      "first": {
        "direction": "column",
        "first": {
          "direction": "row",
          "first": {
            "type": "Image",
            "config": {
              "topic": "/camera_front/color/image_raw",
              "cameraTopic": "/camera_front/color/camera_info",
              "flip": "horizontal"
            }
          },
          "second": {
            "type": "Image",
            "config": {
              "topic": "/camera_front/depth/image_rect_raw",
              "cameraTopic": "/camera_front/depth/camera_info",
              "colorMode": "turbo",
              "flip": "horizontal"
            }
          }
        },
        "second": {
          "direction": "row",
          "first": {
            "type": "Image",
            "config": {
              "topic": "/camera_rear/color/image_raw",
              "cameraTopic": "/camera_rear/color/camera_info",
              "flip": "horizontal"
            }
          },
          "second": {
            "type": "Image",
            "config": {
              "topic": "/camera_rear/depth/image_rect_raw",
              "cameraTopic": "/camera_rear/depth/camera_info",
              "colorMode": "turbo",
              "flip": "horizontal"
            }
          }
        }
      },
      "second": {
        "direction": "column",
        "first": {
          "type": "Plot",
          "config": {
            "paths": [
              {
                "value": "/system/temperature/cpu",
                "enabled": true,
                "color": "#ff0000"
              },
              {
                "value": "/system/temperature/gpu",
                "enabled": true,
                "color": "#00ff00"
              }
            ],
            "minYValue": 0,
            "maxYValue": 100
          }
        },
        "second": {
          "type": "Plot",
          "config": {
            "paths": [
              {
                "value": "/system/cpu/usage",
                "enabled": true,
                "color": "#0000ff"
              },
              {
                "value": "/system/memory/usage",
                "enabled": true,
                "color": "#ff00ff"
              }
            ],
            "minYValue": 0,
            "maxYValue": 100
          }
        },
        "third": {
          "type": "RawMessages",
          "config": {
            "topic": "/realsense/status"
          }
        }
      }
    }
  },
  "globalVariables": {},
  "userNodes": {},
  "playbackConfig": {
    "speed": 1.0
  }
}
